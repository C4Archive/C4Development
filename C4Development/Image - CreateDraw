#import "CFACanvas.h"

CFAImage *myImage;

@implementation CFACanvas
-(void)setup {
[self windowWidth:400 andHeight:320];
myImage = [CFAImage imageName:@"C4Wood" andType:@"png"];
}
-(void)draw {
[myImage drawAt:NSZeroPoint];
}
@end