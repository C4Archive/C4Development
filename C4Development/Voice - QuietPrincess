#import "CFACanvas.h"

CFAVoice *princess;

@implementation CFACanvas
-(void)setup {
princess = [[[CFAVoice alloc] initWithVoice:PRINCESS] retain];
[princess setVolume:0.05];
[princess speak:@"where are my glass slippers?"];
}
@end